{
  "name": "ride-along",
  "version": "0.1.0",
  "scripts": {
    "build": "babel-node scripts/build.js",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged",
    "start": "babel-node scripts/start.js",
    "test": "babel-node scripts/test.js --env=jsdom",
    "prettier": "prettier --no-semi --trailing-comma es5 --write",
    "tslint": "tslint",
    "check-lighthouse": "babel-node scripts/check-lighthouse.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "@types/classnames": "^2.2.0",
    "@types/googlemaps": "^3.26.14",
    "@types/left-pad": "^1.1.1",
    "@types/lodash": "^4.14.72",
    "@types/react": "^15.0.26",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.43",
    "@types/react-router-dom": "^4.0.4",
    "classnames": "^2.2.5",
    "date-fns": "^1.28.5",
    "left-pad": "^1.1.3",
    "lodash": "^4.17.4",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "querystring": "^0.2.0",
    "react-async-component": "^1.0.0-beta.3",
    "react-display-name": "^0.2.3",
    "react-form": "^1.2.8",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.15.3",
    "typescript-fsa": "^2.3.0",
    "typescript-fsa-reducers": "^0.4.3"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^20.0.0",
    "@types/node": "^8.0.5",
    "@types/react-test-renderer": "^15.5.0",
    "@types/webpack": "^3.0.1",
    "app-root-path": "^2.0.1",
    "autoprefixer": "^7.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chalk": "^2.1.0",
    "cli-highlight": "^1.1.4",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "fbjs-scripts": "^0.8.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^4.0.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.3",
    "jest-enzyme": "^3.2.0",
    "lint-staged": "^4.0.0",
    "node-sass": "^4.5.3",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.5",
    "prettier": "^1.4.1",
    "promise": "^8.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dev-utils": "^3.0.0",
    "react-dom": "^15.5.4",
    "react-error-overlay": "^1.0.6",
    "react-svg-loader": "^1.1.1",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.5",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "sw-precache-webpack-plugin": "^0.11.3",
    "ts-loader": "^2.0.3",
    "tslint": "^5.4.3",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.3",
    "url-loader": "^0.5.8",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.1.0",
    "whatwg-fetch": "^2.0.3",
    "yarn-or-npm": "^2.0.4"
  },
  "private": true,
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)test.ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.sass$": "<rootDir>/config/jest/sassTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.svg$": "<rootDir>/config/jest/svgTransform.js",
      "^(?!.*\\.(sass|json|svg)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yon run prettier",
      "git add",
      "yon run tslint"
    ]
  }
}
