{
    "name": "ride-along",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "react-scripts-ts start",
        "build": "react-scripts-ts build",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject",
        "prettier": "prettier --no-semi --trailing-comma all --write 'src/**/*.{js,ts,tsx,css}'",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,ts,tsx,css}": [
            "yon run prettier",
            "git add"
        ]
    },
    "dependencies": {
        "@types/jest": "^19.2.4",
        "@types/node": "^7.0.27",
        "@types/react": "^15.0.26",
        "@types/react-dom": "^15.5.0",
        "react": "^15.5.4",
        "react-dom": "^15.5.4"
    },
    "devDependencies": {
        "husky": "^0.13.4",
        "lint-staged": "^3.6.0",
        "prettier": "^1.4.1",
        "react-scripts-ts": "2.2.0",
        "yarn-or-npm": "^2.0.4"
    }
}