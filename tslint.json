{
  "rules": {
    "align": [false],
    "ban": false,
    "class-name": true,
    "comment-format": [true, "check-space"],
    "curly": false,
    "eofline": false,
    "forin": true,
    "indent": [false],
    "interface-name": [true, "never-prefix"],
    "jsdoc-format": true,
    "label-position": true,
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ],
    "no-any": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [true, "log", "debug", "time", "timeEnd", "trace"],
    "no-consecutive-blank-lines": [false],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "one-line": [false],
    "quotemark": [false],
    "radix": true,
    "semicolon": [false],
    "switch-default": true,
    "trailing-comma": [false],
    "triple-equals": [true],
    "typedef": [true, "parameter", "property-declaration"],
    "typedef-whitespace": [false],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  },
  "jsRules": {
    "align": [false],
    "ban": false,
    "class-name": true,
    "comment-format": [true, "check-space"],
    "curly": false,
    "eofline": false,
    "forin": true,
    "indent": [false],
    "jsdoc-format": true,
    "label-position": true,
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [true, "log", "debug", "time", "timeEnd", "trace"],
    "no-consecutive-blank-lines": [false],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "no-use-before-declare": true,
    "one-line": [false],
    "quotemark": [false],
    "radix": true,
    "semicolon": [false],
    "switch-default": true,
    "trailing-comma": [false],
    "triple-equals": [true],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  }
}
